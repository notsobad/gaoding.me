    /* newControls drawer */
    #newControls{
	position:relative;
	left:-10px;
	margin-right:-9px;
	top:-2px;
    }

    /* Don't show control tray if editing is disabled. */
    body.disable_editing #newControls{
	display:none;
    }

    #newControls.hovered .panel{
	display:block;
    }

    #newControls .panel .more{
	height:10px;
	width:20px;
	margin-left:25px;
    }

    #newControls #moreControls a.export, #newControls #moreControls a.delete{
	display:none;
    }

    #newControls #moreControls:hover a.export, #newControls #moreControls:hover a.delete{    
	display:block;
    }

    #newControls #moreControls:hover a.more{
	display:none;
    }
    
    #newControls.hovered .handle{
	position:absolute;
	left:100%;
	width:32px;
	height:18px;
	margin-left:-13px;
	background:#ddd;
	border-bottom:1px solid #bbb;
	border-top:1px solid #bbb;
	border-right:1px solid #bbb;
	border-bottom-right-radius:15px;
	-moz-border-radius-bottomright:15px;
	border-top-right-radius:15px;
	-moz-border-radius-topright:15px;
	display:block;
    }

    #newControls .handle{
	display:none;
    }

    .selected > .name #newControls.hovered .handle{
	width:0px;
    }

    .selected > .name #newControls{
	left:2px;
    }

    #newControls .panel{
	display:none;
	font-family:Arial;
	background: #ddd;
	border:1px solid #bbb;
	width: 80px;
	padding:10px 0 5px 15px;
	border-top-left-radius:15px;
	-moz-border-radius-topleft:15px;
	border-bottom-left-radius:15px;
	-moz-border-radius-bottomleft:15px;
	border-bottom-right-radius:15px;
	-moz-border-radius-bottomright:15px;
	margin-bottom:10px;
    }

    #newControls .header{
	color:#fff;
	font-style:italic;
