    .content .contentLink{
	/* Need to set position for z-index to work. */
	position:relative;
	z-index:7;
        text-decoration:underline;
    }

    .content .contentLink:hover{
	cursor:pointer;
    }

    .content.editing .contentLink{
	/* Hide links when content is being edited so they can be clicked into
	   to edit. */
	visibility:hidden;
    }

    .content .contentTag{
	position:relative;
	color:#999;
    }

    /* Make tags transparent like the rest of content when editing. */
    .content.editing .contentTag{
	color:transparent;
    }

    .content .contentTagText{
	text-decoration:underline;
    }

    .content .contentTagClickable{
	/* Need to set position for z-index to work. */
	position:absolute;
	z-index:7;

	/* Overlay clickable area on tag text. */
	top:0;
	left:0;
	width:100%;
	height:100%;

	padding:0 1px;
	margin:0 -1px;
        -moz-border-radius:3px;
        -webkit-border-radius:3px;
        -khtml-border-radius:3px;
	border-radius:3px;

	background-color:#000;
	opacity:0;
	filter: alpha(opacity=0);
	*filter: alpha(opacity=0);
	
    }

    .content .contentTagClickable:hover{
	cursor:pointer;
	opacity:0.05;
    }

    .content.editing .contentTagClickable{
	/* Hide tags when content is being edited so they can be clicked into
	   to edit. */
	visibility:hidden;
    }
    
    .content .contentMatch{
