body{
    padding:0;
    margin:0;
    font-size:18px;
    font-family:Times;
    position:relative;
    color:#333;
    background:#efefef;
}

body.sansSerif{
    font-family: "Helvetica Neue", Arial, Sans-serif;
    font-size:16px;
}

.project, .children{
    display:none;
}

.parent, .selected .project, .selected, .selected .open > .children, .parent > .children, .selected > .children{
    display:block;
}

.project.done{
    display:none;
}

.showCompleted .selected .project.done, .parent.done, .selected.done{
    display:block;
}

body.searching .selected .project, body.searching .selected .project.done{
    display:none;
}

body.searching .selected .project.matches, body.searching .selected .project.uncompletedDescendantMatches{
    display:block;
}


body.searching .selected .project.matches.done, body.searching .selected .project.uncompletedDescendantMatches.done{
    display:none;
}

body.searching.showCompleted .selected .project.matches.done, body.searching.showCompleted .selected .project.uncompletedDescendantMatches.done, body.searching.showCompleted .selected .project.completedDescendantMatches{
    display:block;
}


body.searching .add.lastChild{
    display:none;
}

.editor{
    position:absolute;
    padding:0px;
    z-index:2;
}

.editor.hovered{
    z-index:3;
}

.editor.fixed{
    z-index:1;
}

.editor > textarea {
    display:block;
    background:transparent;
    resize:none;
    outline:0;
    border:0;
    padding:0;
    margin:0;
    width:100%;
    height:100%;
    font-family:inherit;
    font-size:inherit;
    line-height:inherit;
    letter-spacing:inherit;
    color: inherit;
    overflow:hidden;
    position:relative;
    word-wrap:break-word;
}

.mozilla .editor > textarea{
    /* Firefox adds 1 extra pixel to the left and right of textareas. */
    left:-1px;
}

    #visible{
