    .notes{
        display:none;
        position:relative;
    }

    /* Expand notes that are clicked into, selected, or match the current
       search. */
    .noted > .notes > .content.editing, .selected > .notes, body.searching .notes.matches > .content{
	height:auto;
	overflow:visible;
    }

    .open > .notes{
        max-height:none;
    }
    
    .noted > .notes{
        display:block;
        margin:0px 0px 0px 25px;
    }

.content{
    display:block;
    word-wrap:break-word;
    position:relative;
    z-index:6;
}

.content.editing{
    color:transparent !important;
    /* Set z-index:inherit rather than z-index:0 because we need links
       under content to have no "z-index frame", which allows us to
       position them above anything on the page, namely editors. */
    z-index:inherit;
}

.content.hovered{
    z-index:inherit;
}

    
